.container
  .container-wrapper
    .row
      .col-lg-2.col-sm-12
        %a.thumbnail.img-org{href: ""}
          %img{alt: "", "data-src" => "holder.js/100%x180"}/
      .col-lg-10.col-sm-12
        %h1 
          = "#{@user.first_name} #{@user.last_name}"
