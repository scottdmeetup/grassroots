<%if @open_params %>
	<% if @open_projects.empty?%>
		$('#projectsDiv').empty()
  	$('#projectsDiv').append('<p> <%= j render "shared/empty_project_thumbnail" %></p>')
	<% else %>
		$('#projectsDiv').empty()
		"<% @open_projects.each do |project| %>"
			$('#projectsDiv').append('<p> <%= j render "shared/project_thumbnail", project: project %></p>')
		"<% end %>"
	<% end %>
<% elsif @production_params %>
	<% if @in_production_projects.empty? %>
		$('#projectsDiv').empty()
  	$('#projectsDiv').append('<p> <%= j render "shared/empty_project_thumbnail" %></p>')
	<% else %>
		$('#projectsDiv').empty()
	"<% @in_production_projects.each do |project| %>"
		$('#projectsDiv').append('<p> <%= j render "shared/project_thumbnail", project: project %></p>')
	"<% end %>"
	<% end %>
